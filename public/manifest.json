{
  "name": "TabEditor",
  "description": "A beautiful tabbed rich-text editor for your 'new tab' page",
  "version": "0.1.1",
  "author": "Adrian Latorre Crespo",
  "homepage_url": "https://github.com/willylatorre/tabeditor",
  "incognito": "not_allowed",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "img/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background_color": "#694747"
}